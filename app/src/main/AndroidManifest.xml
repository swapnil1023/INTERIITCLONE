<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.interiit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".TableTennis"
            android:label="Table Tennis" />
        <activity
            android:name=".Squash"
            android:label="Squash" />
        <activity
            android:name=".Badminton"
            android:label="Badminton" />
        <activity
            android:name=".Volleyball"
            android:label="Volleyball" />
        <activity
            android:name=".MainActivity"
            android:hardwareAccelerated="true"
            android:label="InterIIT "
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>